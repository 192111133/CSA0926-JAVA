import java.util.*;
import java.io.*;
public class  Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int M = 4;
        int N = 5;
        for (int i = 1; i <= N; i++) {
            System.out.println(i + "x" + M + "=" + (i * M));
        }
        return;
    }
}
